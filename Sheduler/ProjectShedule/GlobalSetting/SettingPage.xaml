<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProjectShedule.GlobalSetting.SettingPage"
             BackgroundColor="{DynamicResource PageBackgroundColor}"
             xmlns:viewModels="clr-namespace:ProjectShedule.GlobalSetting.ViewModels"
             xmlns:views="clr-namespace:ProjectShedule.GlobalSetting.Views"
             x:DataType="viewModels:SettingViewModel"
             Title="{Binding Title}">
    <ContentPage.Content>
        <StackLayout>
            <views:ThemeSettingView/>
            <views:SheduleDeleteQuestionSettingView />
            <views:SheduleEventsSettingView />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>