<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProjectShedule.AppFlyout.Main"
            xmlns:pages="clr-namespace:ProjectShedule.AppFlyout.Views"
            xmlns:myPages="clr-namespace:ProjectShedule.Shedule">
  <FlyoutPage.Flyout>
    <pages:FlyoutPage x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
          <myPages:ShedulePage/>
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>